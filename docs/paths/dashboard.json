{
    "/user/v2/getNotificationSettings": {
        "get": {
            "summary": "Get user notification settings",
            "tags": [
                "User"
            ],
            "security": [
                {
                    "BearerAuth": []
                }
            ],
            "parameters": [
                {
                    "name": "userId",
                    "in": "query",
                    "description": "user id",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ],
            "responses": {
                "200": {
                    "description": "OK"
                },
                "401": {
                    "description": "Unauthorized"
                },
                "404": {
                    "description": "User not found"
                },
                "500": {
                    "description": "Internal server error"
                }
            }
        }
    },
    "/user/v2/getSocials": {
        "get": {
            "summary": "Get user social media",
            "tags": [
                "User"
            ],
            "security": [
                {
                    "BearerAuth": []
                }
            ],
            "parameters": [
                {
                    "name": "userId",
                    "in": "query",
                    "description": "user id",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ],
            "responses": {
                "200": {
                    "description": "OK"
                },
                "401": {
                    "description": "Unauthorized"
                },
                "404": {
                    "description": "User not found"
                },
                "500": {
                    "description": "Internal server error"
                }
            }
        }
    },
    "/user/v2/updateUser": {
        "put": {
            "summary": "Update a specific  user",
            "tags": [
                "User"
            ],
            "security": [
                {
                    "BearerAuth": []
                }
            ],
            "parameters": [
                {
                    "name": "userId",
                    "in": "query",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ],
            "requestBody": {
                "required": true,
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/User"
                        }
                    }
                }
            },
            "responses": {
                "200": {
                    "description": "User Updated successfully"
                },
                "401": {
                    "description": "Unauthorized"
                },
                "404": {
                    "description": "User not found"
                },
                "500": {
                    "description": "Internal server error"
                }
            }
        }
    },
    "/user/v2/updateNotificationSetting": {
        "patch": {
            "summary": "Update user notification preferences",
            "tags": [
                "User"
            ],
            "parameters": [
                {
                    "name": "userId",
                    "in": "path",
                    "description": "ID of the user whose notification preferences will be updated",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ],
            "requestBody": {
                "required": true,
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "resumeStatusUpdates": {
                                    "type": "boolean"
                                },
                                "jobAlerts": {
                                    "type": "boolean"
                                },
                                "internshipAlerts": {
                                    "type": "boolean"
                                },
                                "blogUpdates": {
                                    "type": "boolean"
                                },
                                "reminders": {
                                    "type": "boolean"
                                }
                            },
                            "required": [
                                "resumeStatusUpdates",
                                "jobAlerts",
                                "internshipAlerts",
                                "blogUpdates",
                                "reminders"
                            ]
                        }
                    }
                }
            },
            "responses": {
                "200": {
                    "description": "User notification preferences updated successfully"
                },
                "400": {
                    "description": "Invalid request data"
                },
                "404": {
                    "description": "User not found"
                },
                "500": {
                    "description": "Internal server error"
                }
            }
        }
    },
    "/user/internship": {
        "post": {
            "summary": "Create a new internship application",
            "tags": [
                "Internship"
            ],
            "requestBody": {
                "required": true,
                "content": {
                    "multipart/form-data": {
                        "schema": {
                            "$ref": "#/components/schemas/InternshipApplication"
                        }
                    }
                }
            },
            "responses": {
                "201": {
                    "description": "Internship application created successfully"
                },
                "409": {
                    "description": "Internship application already exists"
                },
                "500": {
                    "description": "Internal server error"
                }
            }
        }
    },
